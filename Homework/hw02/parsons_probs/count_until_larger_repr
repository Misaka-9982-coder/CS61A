def count_until_larger(num): #0given
!BLANK = 0  #1given #blankdis
!BLANK = num % 10  #1given #blankrightest
while !BLANK:  #1given #blanknum
if num % 10 > !BLANK:  #2given #blankrightest
return !BLANK  #3given #blankdis
!BLANK //= !BLANK  #2given #blanknum #blank10
!BLANK += !BLANK  #2given #blankdis #blank1
return !BLANK  #1given #blank-1
print('DEBUG:', !BLANK)
print('DEBUG:', !BLANK)
